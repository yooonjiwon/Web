<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<properties>
		<property name="driver" value="oracle.jdbc.OracleDriver" />
		<property name="url" value="jdbc:oracle:thin:@210.16.214.202:1521:xe" />
		<property name="username" value="dieat" />
		<property name="password" value="1234" />
	</properties>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="${driver}" />
				<property name="url" value="${url}" />
				<property name="username" value="${username}" />
				<property name="password" value="${password}" />
			</dataSource>
		</environment>
	</environments>
	
	<mappers>
	 
	<mapper resource="com/dieat/mapper/member-mapper.xml" />
	<mapper resource="com/dieat/mapper/foodInfo-mapper.xml"/>	 
	<mapper resource="com/dieat/mapper/gym-mapper.xml"/> 
	<mapper resource="com/dieat/mapper/board-mapper.xml"/> 
	<mapper resource="com/dieat/mapper/exercise-mapper.xml"/>
	
	</mappers>
</configuration>