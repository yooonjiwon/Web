	<select id="selectGymList" resultType="com.dieat.dto.GymDto">
				SELECT
					GI_No, GI_name, GI_Locker, GI_Sporting, GI_PT, GI_price, GI_comment, M_ID, GI_DATE, GI_status
				FROM
					GYM
				WHERE
						GI_status = 0
				ORDER BY GI_No DESC
