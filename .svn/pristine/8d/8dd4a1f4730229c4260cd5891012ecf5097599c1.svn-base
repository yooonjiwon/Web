<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.dieat.mapper.GymMapper">

	<insert id="insertGym" parameterType="com.dieat.dto.Gym">
		INSERT INTO GYM (gymno, memberid, gymname, gymlocker, gymsporting, gympt, gymprice, gymcomment, gymdate ,gymstatus)
		VALUES 
		(
			gym_sequence.nextval, 
			#{M_id}, #{gi_name}, #{gi_locker}, #{gi_sporting}, #{gi_pt}, #{gi_price}, #{gi_comment}, #{gi_date}, 0}, 
			gym_sequence.currval, 1
		)
	</insert>

</mapper>








