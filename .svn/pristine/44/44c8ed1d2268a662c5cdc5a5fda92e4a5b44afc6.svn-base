function fn_save(){
	$.ajax({
		type : "post",
		url: '/fnc/guidebook/action.json',
		data : $("#frmGuidBook").serialize(),
		dataType: 'json',
		success: function(data) {
			if(data && data.result == "SUCC"){
				$('.deemed-white').fadeIn(200);
			}else{
				alert(data.resultMsg);
			}
		},
		error: function(data, status, err) {
		}
	});
}

$(document).ready(function(){
	$.validator.addMethod("tyCheck", function(v, e) {

		if($("#guidebook_type1").val() !="" || $("#guidebook_type2").val() !=""){
			return true;
		}
		return false;
	});

	$.validator.addMethod("telCheck", function(v, e) {
		return this.optional(e) || /^(01[016789]{1}|02|0[3-9]{1}[0-9]{1})-?[0-9]{3,4}-?[0-9]{4}$/g.test(v);
	});

	$("#frmGuidBook").validate({
		ignore: "input[type='text']:hidden",
		rules : {
			tyArr : { tyCheck : true },
			name : { required : true },
			email : { required : true, email : true },
			telno : { required : true, telCheck : true },
			zip : { required : true },
			adres1 : { required : true },
			adres2 : { required : true },
			guidebook_agree : { required : true }
		},
		messages : {
			tyArr : { tyCheck : "가이드북 또는 지도중 1권 이상 선택해주세요" },
			telno : { telCheck : "유효하지 않은 전화번호입니다."},
		},
		errorElement: 'div',
		errorClass: 'ess_message',
		errorPlacement: function(error, element) {

			if(element.context.type == "checkbox"){
				error.insertAfter(element.parent().parent().find(':last'));
			}else if(element.context.type == "select-one"){
				//error.insertAfter(element.parent().parent().find(':last'));
			}else{
				error.insertAfter(element.parent());
			}
		},
		highlight:function(element, errorClass, validClass) {
			$(element).parent().parent().addClass("ess_message");
			$(element).addClass('ess_message');
		},
		unhighlight: function(element, errorClass, validClass) {
			$(element).parent().parent().removeClass("ess_message");
			$(element).removeClass('ess_message');
		},
		submitHandler: function (frm) {
			if(confirm("저장 하시겠습니까?")){
				fn_save();
				return false;
			}else{
				return false;
			}
		}
	});

	$("#btnCancel").click(function(){
		window.history.back();
	});

});
