#container #pageheader.search .holder {padding-right:0 !important; max-width:1000px !important;}

/* footer */
#footer {font-size:13px;}
#footer .top .holder {padding:23px 0; border-top:4px solid #626262; font-weight:500;}
#footer .top .holder strong {display:block; padding-left:10px; font-size:1.077em; color:#111;}
#footer .top .holder p {margin-top:6px; padding-left:10px; color:#a7a7a7; color:#666;}
#footer .address, #footer .contact, #footer .tourism {float:left; width:30%;}
#footer .contact {width:40%;}
#footer .bottom {background:#ebebeb; background:#f4f2f2;}
#footer .bottom .holder {position:relative; padding:18px 10px 20px 0px;}
#footer .bottom h2 {float: left; width: 24.0em;}
#footer .bottom .newsletter, #footer .bottom .review {position:relative; overflow: hidden;}
#footer .bottom .review {/*float:right;*/}
#footer .bottom .newsletter p, #footer .bottom .review p {display:inline-block; width:167px; float:right;}
#footer .bottom .review p {width:167px;}
#footer .bottom .newsletter a, #footer .bottom .review a {display:block; position:relative; padding:1px 30px 3px 23px; border:2px solid #bbb; background:#bbb; vertical-align:middle; font-size:14px; color:#fff; border:2px solid #6b6b6b; background:#6b6b6b;}
html.ja #footer .bottom .newsletter a, html.ja #footer .bottom .review a {font-size: 13px; letter-spacing: -2px;}
#footer .bottom .newsletter a [class*="ion"], #footer .bottom .review a [class*="ion"] {position:absolute; top:4px; right:14px; font-size:0.875em;}
#footer .bottom dl {display:inline-block; margin-top:21px;}
#footer .bottom dd {position:relative; float:left; margin-right:8px; padding-right:9px;}
#footer .bottom dd:after {content:" "; position:absolute; top:50%; right:0; margin-top:-4px; width:0; height:10px; overflow:hidden; border-right:1px solid #bbb;}
#footer .bottom dd:nth-last-child(1) {margin-right:0; padding-right:0;}
#footer .bottom dd:nth-last-child(1):after {display:none;}
#footer .copyright {border-top:1px solid #e1e1e1; padding:19px 10px; text-align:center;}
#footer .bottom dd a{color:#666;}
.accessibility img {max-width: 100%;height: auto;}

/*20160727 lsh*/
.snsarea_mobile{}

.fselect_area {float:left; width:320px; width:26.0em; margin-right:3.0em;}
.fmenu_area {float:right; width:200px; width:23.4em;}
#footer .bottom .newsletter strong, #footer .bottom .review strong {float:left;}
#footer .bottom .newsletter {margin-bottom:4px;}
.fmenu_area a.accessibility {display:inline-block; float:right; width:90px;}

