.camera_prev, .camera_next {
	display: block;
	position: absolute;
	width: 21px;
	height: 21px;
	left: 50%;
	color: #cdcdce;
	cursor: pointer;
	bottom: 22px;
	margin-left: -280px;
	z-index: 999;
	background: url(../images/prevnext.png) 0 0 no-repeat;
	opacity: 1;
	transition: width 0.5s ease;
	-o-transition: width 0.5s ease;
	-webkit-transition: width 0.5s ease;
	
}



.camera_next {
	margin-left: 260px;
	background-position: right 0;
}

.camera_prev:hover,
.camera_next:hover  {
